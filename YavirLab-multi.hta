If Not file.AtEndOfStream Then
tempFile.WriteLine file.ReadLine
End If
Do Until file.AtEndOfStream
line = Trim(file.ReadLine)
If line <> "" Then
fields = Split(line, vbTab)
If UBound(fields) >= 7 Then
If Trim(fields(1)) = username Then
